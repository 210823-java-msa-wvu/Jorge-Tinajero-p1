<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>dev.tinajero.servlets.LoginServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Benco</servlet-name>
    <servlet-class>dev.tinajero.servlets.BencoServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Employee</servlet-name>
    <servlet-class>dev.tinajero.servlets.EmployeeServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>DepartmentHead</servlet-name>
    <servlet-class>dev.tinajero.servlets.DepHServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>DirectSupervisor</servlet-name>
    <servlet-class>dev.tinajero.servlets.DirectSuperServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>GradeChange</servlet-name>
    <servlet-class>dev.tinajero.servlets.GradeChangeServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>GradeChange</servlet-name>
    <url-pattern>/gradechange</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>DirectSupervisor</servlet-name>
    <url-pattern>/directsupervisor</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>DepartmentHead</servlet-name>
    <url-pattern>/departmenthead</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>Employee</servlet-name>
    <url-pattern>/employeereimbursement</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Benco</servlet-name>
    <url-pattern>/benco</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>


</web-app>
